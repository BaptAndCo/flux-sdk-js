{
  "extends": "airbnb/base",
  "parserOptions": {
    "ecmaFeatures": {
      "experimentalObjectRestSpread": true
    }
  },
  "rules": {
    "space-before-function-paren": [2, "never"],
    "no-use-before-define": ["error", { "functions": false }],
    "max-len": 1,
    "no-param-reassign": 0
  }
}
