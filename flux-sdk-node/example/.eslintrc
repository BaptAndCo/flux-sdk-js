{
  "extends": "airbnb/legacy",
  "env": {
    "node": true,
    "es6": false
  },
  "rules": {
    "space-before-function-paren": [2, "never"],
    "no-var": 0,
    "prefer-arrow-callback": 0,
    "object-shorthand": 0,
    "vars-on-top": 0,
    "func-names": 0,
    "prefer-template": 0,
    "no-console": 0,
    "no-param-reassign": 0
  }
}
